{
  "name": "near-bindgen",
  "version": "0.0.0",
  "description": "Tests for near bindings",
  "main": "test.js",
  "scripts": {
    "build:as": "../../bin/asc assembly/test.ts --validate --runtime stub --binaryFile out/test.wasm --textFile out/test.wat --runPasses dce --debug",
    "build:hello": "../../bin/asc assembly/hello/main.ts --validate --runtime stub --binaryFile out/hello.wasm --textFile out/hello.wat --runPasses dce --debug",
    "build": "npm run build:as && npm run build:hello",
    "test": "node --experimental-wasm-bigint ./test.js"
  }
}
